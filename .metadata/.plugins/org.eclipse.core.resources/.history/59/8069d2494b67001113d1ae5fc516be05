package com.christine.cart.sqlite;

public class GroceryItem {
	Integer _id;
	String _username;
	String _itemname;
	int _calories;
	
	public GroceryItem(){
		
	}
	
	public GroceryItem(int id, String username, String itemname, int calories){
		this(null, username, itemname, calories);
	}
	public GroceryItem(int id, String username, String itemname, int calories){
		_id = id;
		_username = username;
		_itemname = itemname;
		_calories = calories;
	}
}
