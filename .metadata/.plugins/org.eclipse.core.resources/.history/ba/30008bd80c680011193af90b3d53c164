package com.christine.cart.sqlite;

public class GroceryItem {
	Integer _id;
	String _username;
	String _itemname;
	int _calories;
	int _protein;
	int _totalfats;
	int _carbs;
	int _fiber;
	int _sugar;
	int _calcium;
	int _potassium;
	int _sodium;
	int _zinc;
	int _vitC;
	int _vitB6;
	int _vitB12;
	int _vitA;
	int _vitE;
	int _vitK;
	int _fatSat;
	int _fatMono;
	int _fatPoly;
	int _cholesterol;
	int _servingWeight;
	int _serving;
	
	public GroceryItem(){
		
	}
	
	public GroceryItem(String username, String itemname, int calories){
		this(null, username, itemname, calories);
	}
	public GroceryItem(Integer id, String username, String itemname, int calories, int protein, 
						int totalfats, int carbs, int fiber, int sugar, int calcium, int potassium, int sodium,
						int zinc, int vitC, int vitB6, int vitB12, int vitA, int vitE, int vitK, int fatSat,
						int fatMono, int fatPoly, int cholesterol, int servingWeight, int serving){
		_id = id;
		_username = username;
		_itemname = itemname;
		_calories = calories;
		_protein = protein;
		_totalfats = totalfats;
		_carbs = carbs;
		_fiber = fiber;
		_sugar = sugar;
		_calcium = calcium;
		_potassium = potassium;
		_sodium = sodium;
		_zinc = zinc;
		_vitC = vitC;
		_vitB6 = vitB6;
		_vitB12 = vitB12;
		_vitA = vitA;
		_vitE = vitE;
		_vitK = vitK;
		_fatSat = fatSat;
		_fatMono = fatMono;
		_fatPoly = fatPoly;
		_cholesterol = cholesterol;
		_servingWeight = servingWeight;
		_serving = serving;
	}
	
	// get Id
	public int getId(){
		return this._id;
	}
	
	// set Id
	public void setId(int id){
		this._id = id;
	}
	
	// get username
	public String getUsername(){
		return this._username;
	}
	
	// set username
	public void setUsername(String username){
		this._username = username;
	}

	// get name
	public String getItemName(){
		return this._itemname;
	}
	
	// set name
	public void setItemName(String itemname){
		this._itemname = itemname;
	}
	
	// get calories
	public int getCalories(){
		return this._calories;
	}
	
	// set Id
	public void setCalories(int calories){
		this._calories = calories;
	}
	
	
}
